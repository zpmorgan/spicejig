<!DOCTYPE HTML>
<html>
  <head>
    <script>
      var require = {
        //baseUrl: "/",
        deps: ['domReady', 'phaser', 'game', 'puzzle'],
        paths: {
          js: 'foo',
          domReady: '/js/domReady',
          phaser: '/js/phaser',
          game: '/skrpt/game',
          puzzle: '/skrpt/puzzle',
        },
        callback: function(domready,ph,ga,pu){
          STARTWHENREADY(domready);
        },
      };
    </script>

    <!--<script data-main="/skrpt/game.js" src="/js/require.js"></script>-->
		<script src="/js/require.js"></script>
    <title>{{title}}</title>
    <style>
      body {
        margin: 0px;
        padding: 0px;
      }
      div.imposition{
        position: absolute;

        transform: translate(-50%, -50%); /* This is a shorthand of
                                             translateX(-50%) and translateY(-50%) */
        background: rgba(44,44,44, .8);
        border-radius: 10px;
        padding: 15px;
        color : #ffffff;
      }
      div.imposition a.menu {
        color: rgb(111,222,111);
        text-decoration: underline;
        cursor: pointer;
      }
      div.center{
        top: 50%;  /* position the top  edge of the element at the middle of the parent */
        left: 50%; /* position the left edge of the element at the middle of the parent */
      }
      div.corner{
        top: 2%;  /* position the top  edge of the element at the middle of the parent */
        left: 2%; /* position the left edge of the element at the middle of the parent */
      }
      a.boxclose{
        float:right;
        margin-top:-30px;
        margin-right:-30px;
        cursor:pointer;
        color: #fff;
        border: 1px solid #AEAEAE;
        border-radius: 30px;
        background: #605F61;
        font-size: 31px;
        font-weight: bold;
        display: inline-block;
        line-height: 0px;
        padding: 11px 3px;
      }
      .boxclose:before {
        content: "Ã—";
      }
      a:link {
        color: orange;
      }
      a:visited {
        color: yellowgreen;
      }
      a.menu {
      }
      a.menu-option {
        display: block;
        font-size: x-large;
        background:#eee;
        text-decoration:none;
        color:#333;
        font-family:Arial, sans-serif;
        /*font-size:11px;*/
        font-weight:bold;
        padding:3px 5px;
        border:3px solid #aaa;
        border-radius:6px;
        cursor:default;
      }
      a.menu-option:hover {
        background-color:#f2f2f2;
        border-color:#888;
        box-shadow:0 0 5px #ccc;
      }
      a.menu-option:active {
        /*vertical-align:-3px;*/
      }
    </style>
    <script> //global stuff G
      var G = {};
      G.spec = {{&spec}};
      G.env = "{{env}}";
    </script>
  </head>
  <body>
  </body>
</html>
